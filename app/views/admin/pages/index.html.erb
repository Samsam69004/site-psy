<h1>Pages du site</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Slug</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @pages.each do |page| %>
      <tr>
        <td><%= page.title %></td>
        <td><%= page.slug %></td>
        <td><%= link_to "Modifier", edit_admin_page_path(page) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
